<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], title: 'Scheduler Alert', sticky: false)  %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], title: 'Scheduler Error', sticky: false)  %>
<% else %>
  <%= js add_gritter(flash[:notice], title: 'Scheduler Notification', sticky: false)  %>
<% end %>
